{
    "defaultSeverity": "error",
    "extends": [],
    "jsRules": {},
    "rules": {
        "no-any": true,
        "new-parens": true,
        "no-conditional-assignment": true,
        "no-namespace": true,
        "no-parameter-reassignment": true,
        "no-var-requires": true,
        "only-arrow-functions": true,
        "no-arg": true,
        "no-switch-case-fall-through": true,
        "member-ordering": [
            true,
            {
                "order": [
					"error",
                    "public-before-private",
                    "variables-before-functions",
                    "static-before-instance"
                ]
            }
		],
		"curly": "",
		"brace-style": [
			"allman",
			{
			  "allowSingleLine": true
			}
		  ],
        "indent": [ true, "spaces", 4],
		"typedef": [true, "arrow-call-signature", "parameter", "variable-declaration", "arrow-parameter", "property-declaration", "array-destructuring", "object-destructuring", "member-variable-declaration"],
		"newline-before-return": true,
        "triple-equals": true,
        "no-duplicate-switch-case": true,
        "switch-default": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-eval": true,
        "no-invalid-template-strings": true,
        "no-invalid-this": true,
        "no-shadowed-variable": true,
        "no-this-assignment": true,
        "no-unused-expression": true,
        "no-var-keyword": true,
        "radix": true,
        "use-isnan": true,
        "max-classes-per-file": [ true, 2 ],
        "no-default-export": true,
        "no-duplicate-imports": true,
        "no-require-imports": true,
        "trailing-comma": false,
        "arrow-parens": true,
        "binary-expression-operand-order": true,
        "class-name": true,
        "no-consecutive-blank-lines": true,
        "no-irregular-whitespace": true,
        "import-spacing": true,
        "one-variable-per-declaration": true,
        "prefer-function-over-method": true,
        "prefer-template": true,
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "space-before-function-paren": [
            true,
            "never"
        ],
        "space-within-parens": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-module",
            "check-separator",
            "check-rest-spread",
            "check-type",
            "check-typecast",
            "check-type-operator",
            "check-preblock"
        ]
    },
    "file-header": [
        true,
        "Copyright \\d{4} AB Regin",
        "Copyright 2018 AB Regin"
    ],
    "rulesDirectory": []
}
